<Window x:Class="SSHF_WPFTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" mc:Ignorable="d"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:l="clr-namespace:SSHF_WPFTest" 
        xmlns:vm="clr-namespace:SSHF_WPFTest.ViewModels"
        Title="MainWindow" Height="450" Width="800"
        Background="Transparent"
        WindowStyle="None" 
        AllowsTransparency="True"
        MouseLeftButtonDown="Window_MouseLeftButtonDown">


    <Window.DataContext>
        <vm:MainWindowViewModel/>
    </Window.DataContext>


    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Views/Windows/MainWindowRes/F_Logo.png" Stretch="Uniform"></ImageBrush>
        </Grid.Background>
    </Grid>
</Window>
